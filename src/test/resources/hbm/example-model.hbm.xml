<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="org.r7c.testl.model.xml.ExampleModelHbm"
           table="EXAMPLE_MODEL_HBM">
        <id name="Id" type="java.math.BigInteger">
            <column name="ID" scale="0"/>
            <generator class="sequence">
                <param name="sequence_name">SEQ_EXAMPLE_MODE_HBM</param>
            </generator>
        </id>
        <property column="STRING_VALUE" name="StringValue" type="java.lang.String" length="64" index="idx_hbm_string_value"/>
    </class>
</hibernate-mapping>